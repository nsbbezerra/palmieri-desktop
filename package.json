{
  "name": "palmieri-uniformes",
  "version": "0.1.0",
  "private": true,
  "author": {
    "email": "contato.nk.info@gmail.com",
    "name": "NK InformÃ¡tica - Natanael Bezerra",
    "url": "https://github.com/nsbbezerra"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "keywords": [
    "Example",
    "React",
    "Electron"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "is-electron": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-lottie": "^1.2.3",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-select": "^3.1.0"
  },
  "scripts": {
    "dev": "yarn react-scripts start",
    "build": "react-scripts build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "package-win": "electron-builder build --ia32 --x64 --win nsis -c.extraMetadata.main=build/electron.js --publish never",
    "package-linux": "electron-builder build --linux appimage --x64 -c.extraMetadata.main=build/electron.js --publish never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^8.2.4",
    "electron-builder": "^22.5.1",
    "wait-on": "^4.0.2"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "palmieri",
      "owner": "nkinformatica"
    },
    "win": {
      "icon": "build/favicon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icone512.png",
      "category": "Office"
    }
  }
}
